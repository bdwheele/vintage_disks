comment: 'Kaypro 5.25" 40-track 9-sector SSDD'

image_reader {
	filename: "kaypro.img"
	img {}
}

image_writer {
	filename: "kaypro.img"
	img {}
}

layout {
	tracks: 40
	sides: 1
	layoutdata {
		sector_size: 256
		physical {
			start_sector: 0
			count: 10
		}
	}
}

encoder {
	ibm {
		trackdata {
			target_rotational_period_ms: 167
			target_clock_period_us: 3.333
		}
	}
}

decoder {
	ibm {}
}

tpi: 48




